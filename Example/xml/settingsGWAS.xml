            <global_key>triosNumber_initial</global_key>
        </mapping>
        <mapping method="EXTRACTCASES">
            <source_key>replication_sample_size</source_key>
            <global_key>caseNumber_replicate</global_key>
        </mapping>
        <mapping method="EXTRACTCONTROLS">
            <source_key>replication_sample_size</source_key>
            <global_key>controlNumber_replicate</global_key>
        </mapping>
        <mapping method="EXTRACTINDIVIDUALS">
            <source_key>replication_sample_size</source_key>
            <global_key>individualNumber_replicate</global_key>
        </mapping>
        <mapping method="EXTRACTTRIOS">
            <source_key>replication_sample_size</source_key>
            <global_key>triosNumber_replicate</global_key>
        </mapping>
        <mapping>
            <source_key>study_accession</source_key>
            <global_key>sourceId</global_key>
        </mapping>
    </table>

